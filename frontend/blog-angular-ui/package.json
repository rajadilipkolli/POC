{
  "name": "blog-angular-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dist",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "test:ci": "cross-env SKIP_BROWSER_TESTS=true ng test --watch=false --browsers=ChromeHeadlessCustom --karma-config=karma.conf.js || (echo 'Tests failed' && exit 1)",
    "test:watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ci": "npm run clean && npm run test:ci && npm run build:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.3.4",
    "@angular/common": "20.3.4",
    "@angular/compiler": "20.3.4",
    "@angular/core": "20.3.4",
    "@angular/forms": "20.3.4",
    "@angular/localize": "20.3.4",
    "@angular/platform-browser": "20.3.4",
    "@angular/platform-browser-dynamic": "20.3.4",
    "@angular/router": "20.3.4",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.3.5",
    "@angular/cli": "20.3.5",
    "@angular/compiler-cli": "20.3.4",
    "@angular/language-service": "20.3.4",
    "@types/jasmine": "5.1.9",
    "@types/jasminewd2": "2.0.13",
    "@types/node": "22.18.10",
    "angular-eslint": "20.4.0",
    "cross-env": "^10.0.0",
    "eslint": "9.37.0",
    "jasmine-core": "5.12.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "protractor": "7.0.0",
    "puppeteer": "24.24.0",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.0"
  }
}
