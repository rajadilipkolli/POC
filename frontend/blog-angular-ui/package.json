{
  "name": "blog-angular-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dist",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "test:ci": "cross-env SKIP_BROWSER_TESTS=true ng test --watch=false --browsers=ChromeHeadlessCustom || echo 'Tests skipped in CI environment'",
    "test:watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ci": "npm run clean && npm run test:ci && npm run build:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.10",
    "@angular/common": "19.2.10",
    "@angular/compiler": "19.2.10",
    "@angular/core": "19.2.10",
    "@angular/forms": "19.2.10",
    "@angular/localize": "19.2.10",
    "@angular/platform-browser": "19.2.10",
    "@angular/platform-browser-dynamic": "19.2.10",
    "@angular/router": "19.2.10",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.12",
    "@angular/cli": "19.2.12",
    "@angular/compiler-cli": "19.2.10",
    "@angular/language-service": "19.2.10",
    "@types/jasmine": "5.1.8",
    "@types/jasminewd2": "2.0.13",
    "@types/node": "22.15.17",
    "cross-env": "^7.0.3",
    "jasmine-core": "5.7.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "protractor": "7.0.0",
    "puppeteer": "24.8.2",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  }
}
